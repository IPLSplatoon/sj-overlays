{
    "name": "sj-overlays",
    "version": "3.0.0",
    "compatibleDashboardVersion": "^4.4.0",
    "compatibleGameVersions": [
        "SPLATOON_2",
        "SPLATOON_3"
    ],
    "description": "Broadcast overlays for SuperJump, a Splatoon tournament by Inkling Performance Labs.",
    "homepage": "https://github.com/iplsplatoon/sj-overlays",
    "author": {
        "name": "inkfarer",
        "email": "inkfarer@inkfarer.com",
        "url": "https://inkfarer.com"
    },
    "contributors": [
        "BrushStrokes (https://brush.rip/)",
        "TheMoo (https://twitter.com/TheMoo_spl)"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack",
        "build:clean": "npm run clean && npm run build",
        "build:dev": "webpack",
        "clean": "trash node_modules/.cache && trash dashboard && trash graphics",
        "start": "node ../..",
        "watch": "webpack -w",
        "lint": "eslint ./src --ext .js,.vue,.ts",
        "fix": "eslint ./src --ext .js,.vue,.ts --fix"
    },
    "nodecg": {
        "compatibleRange": "^1.7.0 || ^2.0.0",
        "mount": [
            {
                "directory": "assets",
                "endpoint": "assets"
            }
        ],
        "dashboardPanels": [
            {
                "name": "bracketConfig",
                "title": "Bracket Config",
                "width": 2,
                "file": "bracketConfig.html",
                "workspace": "ipl",
                "headerColor": "#3d1a4b"
            }
        ],
        "graphics": [
            {
                "file": "main.html",
                "width": 1920,
                "height": 1080
            },
            {
                "file": "break.html",
                "width": 1920,
                "height": 1080
            },
            {
                "file": "stats.html",
                "width": 1920,
                "height": 1080
            },
            {
                "file": "bracket.html",
                "width": 1920,
                "height": 1080
            }
        ]
    },
    "devDependencies": {
        "@fortawesome/fontawesome-svg-core": "6.1.1",
        "@fortawesome/free-brands-svg-icons": "^6.1.1",
        "@fortawesome/free-solid-svg-icons": "6.1.1",
        "@fortawesome/vue-fontawesome": "^3.0.0-5",
        "@iplsplatoon/vue-components": "^2.10.0",
        "@nodecg/types": "^2.1.11",
        "@pixi/app": "^6.1.2",
        "@pixi/constants": "^6.1.2",
        "@pixi/core": "^6.1.2",
        "@pixi/display": "^6.1.2",
        "@pixi/loaders": "^6.1.2",
        "@pixi/math": "^6.1.2",
        "@pixi/runner": "^6.1.2",
        "@pixi/settings": "^6.1.2",
        "@pixi/sprite": "^6.1.2",
        "@pixi/spritesheet": "^6.1.2",
        "@pixi/ticker": "^6.1.2",
        "@pixi/utils": "^6.1.2",
        "@tourneyview/common": "0.1.0",
        "@tourneyview/importer": "0.1.0",
        "@tourneyview/renderer": "0.1.0",
        "@types/d3": "^7.4.0",
        "@types/lodash": "^4.14.171",
        "@types/luxon": "^1.27.1",
        "@types/node": "^15.14.9",
        "@types/webpack": "^5.28.0",
        "@typescript-eslint/eslint-plugin": "^5.59.1",
        "@typescript-eslint/parser": "^5.59.1",
        "@vue/compiler-sfc": "^3.2.31",
        "cross-env": "^7.0.3",
        "css-loader": "^6.2.0",
        "d3": "^7.6.1",
        "eslint": "^8.49.0",
        "eslint-import-resolver-typescript": "^3.6.0",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-vue": "^9.17.0",
        "fork-ts-checker-webpack-plugin": "^7.2.3",
        "globby": "^11.0.3",
        "gsap": "npm:@gsap/shockingly@^3.10.2",
        "html-webpack-plugin": "^5.5.0",
        "lodash": "^4.17.21",
        "luxon": "^2.0.1",
        "pinia": "^2.0.16",
        "pixi-filters": "^4.1.4",
        "sass": "^1.49.10",
        "sass-loader": "^12.6.0",
        "style-loader": "^3.3.1",
        "trash-cli": "^4.0.0",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.7.0",
        "tsconfig-paths-webpack-plugin": "^3.5.2",
        "typescript": "^5.2.2",
        "vue": "^3.2.33",
        "vue-eslint-parser": "^9.3.1",
        "vue-loader": "^17.0.0",
        "webpack": "^5.72.0",
        "webpack-cli": "^4.9.2",
        "webpack-livereload-plugin": "^3.0.2",
        "webpack-node-externals": "^3.0.0"
    }
}
