{
    "name": "sj-overlays",
    "version": "3.0.0",
    "compatibleDashboardVersion": "^4.18.0",
    "compatibleGameVersions": [
        "SPLATOON_2",
        "SPLATOON_3"
    ],
    "description": "Broadcast overlays for SuperJump, a Splatoon tournament by Inkling Performance Labs.",
    "homepage": "https://github.com/iplsplatoon/sj-overlays",
    "author": {
        "name": "inkfarer",
        "email": "inkfarer@inkfarer.com",
        "url": "https://inkfarer.com"
    },
    "contributors": [
        "BrushStrokes (https://brush.rip/)",
        "TheMoo (https://twitter.com/TheMoo_spl)"
    ],
    "scripts": {
        "build:clean": "run-s clean build",
        "build": "run-p build:browser build:extension",
        "build:browser": "vite build",
        "build:extension": "tsc -b tsconfig.extension.json",
        "clean": "trash node_modules/.cache && trash dashboard && trash graphics && trash shared && trash extension",
        "start": "node ../..",
        "watch": "run-p watch:*",
        "watch:browser": "vite",
        "watch:extension": "tsc -b tsconfig.extension.json -w",
        "lint": "eslint ./src --ext .js,.vue,.ts",
        "fix": "eslint ./src --ext .js,.vue,.ts --fix"
    },
    "nodecg": {
        "compatibleRange": "^1.7.0 || ^2.0.0",
        "mount": [
            {
                "directory": "assets",
                "endpoint": "assets"
            }
        ],
        "dashboardPanels": [
            {
                "name": "sjConfig",
                "title": "SuperJump Config",
                "width": 3,
                "file": "sjConfig.html",
                "workspace": "ipl",
                "headerColor": "#3d1a4b"
            },
            {
                "name": "centralTeamSearchDialog",
                "dialog": true,
                "file": "centralTeamSearchDialog.html",
                "title": "",
                "width": 4
            }
        ],
        "graphics": [
            {
                "file": "main.html",
                "width": 1920,
                "height": 1080
            },
            {
                "file": "break.html",
                "width": 1920,
                "height": 1080
            },
            {
                "file": "stats.html",
                "width": 1920,
                "height": 1080
            },
            {
                "file": "bracket.html",
                "width": 1920,
                "height": 1080
            },
            {
                "file": "replayBug.html",
                "width": 1920,
                "height": 1080
            }
        ],
        "bundleDependencies": {
            "ipl-overlay-controls": "^4.18.0"
        }
    },
    "dependencies": {
        "@tourneyview/common": "0.6.0",
        "axios": "^1.7.2",
        "jose": "^5.6.3"
    },
    "devDependencies": {
        "@fortawesome/fontawesome-svg-core": "6.1.1",
        "@fortawesome/free-brands-svg-icons": "^6.1.1",
        "@fortawesome/free-solid-svg-icons": "6.1.1",
        "@fortawesome/vue-fontawesome": "^3.0.0-5",
        "@iplsplatoon/vue-components": "^3.3.3",
        "@nodecg/types": "^2.1.11",
        "@pixi/app": "^6.1.2",
        "@pixi/constants": "^6.1.2",
        "@pixi/core": "^6.1.2",
        "@pixi/display": "^6.1.2",
        "@pixi/loaders": "^6.1.2",
        "@pixi/math": "^6.1.2",
        "@pixi/runner": "^6.1.2",
        "@pixi/settings": "^6.1.2",
        "@pixi/sprite": "^6.1.2",
        "@pixi/spritesheet": "^6.1.2",
        "@pixi/ticker": "^6.1.2",
        "@pixi/utils": "^6.1.2",
        "@tourneyview/renderer": "0.6.0",
        "@types/d3": "^7.4.0",
        "@types/lodash": "^4.14.171",
        "@types/luxon": "^1.27.1",
        "@types/node": "^20.14.10",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^5.59.1",
        "@typescript-eslint/parser": "^5.59.1",
        "@vitejs/plugin-vue": "5.2.4",
        "@vue/compiler-sfc": "3.5.17",
        "cross-env": "^7.0.3",
        "d3": "^7.6.1",
        "eslint": "^8.49.0",
        "eslint-import-resolver-typescript": "^3.6.0",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-vue": "^9.17.0",
        "globby": "^11.0.3",
        "gsap": "npm:@gsap/shockingly@^3.10.2",
        "lodash": "^4.17.21",
        "luxon": "^2.5.2",
        "npm-run-all": "^4.1.5",
        "pinia": "^2.0.16",
        "pixi-filters": "^4.1.4",
        "sass": "^1.49.10",
        "trash-cli": "^4.0.0",
        "ts-node": "^10.7.0",
        "typescript": "5.5.4",
        "uuid": "^11.1.0",
        "vite": "^5.3.3",
        "vite-plugin-checker": "0.9.3",
        "vite-plugin-nodecg": "github:inkfarer/vite-plugin-nodecg#4e7eeea",
        "vite-tsconfig-paths": "5.1.4",
        "vue": "3.5.17",
        "vue-eslint-parser": "10.1.3",
        "vue-tsc": "2.2.10"
    }
}
